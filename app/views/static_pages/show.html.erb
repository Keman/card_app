<h1>Все карточки</h1>
 
<table>
  <tr>
    <th>Оригинал</th>
    <th>Перевод</th>
    <th>Дата просмотра</th>
  </tr>
 
  <% @cards.each do |card| %>
    <tr>
      <td><%= card.original_text %></td>
      <td><%= card.translated_text %></td>
      <td><%= card.review_date.to_formatted_s(:slash) %></td>
    </tr>
  <% end %>
</table>
